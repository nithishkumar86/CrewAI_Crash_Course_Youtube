    "from pydantic import BaseModel\n",
    "\n",
    "class Student(BaseModel):\n",
    "    id: int\n",
    "    name: str\n",
    "\n",
    "# Create instance\n",
    "student = Student(id=1, name=\"John\")\n",
    "\n",
    "print(student)\n",
    "# Convert to dictionary\n",
    "print(student.model_dump())\n",
    "# Output: {'id': 1, 'name': 'John'}"
   ]
