    "import os\n",
    "from dotenv import load_dotenv\n",
    "load_dotenv()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "2d9033d9",
   "metadata": {},
   "outputs": [],
   "source": [
    "from crewai import Agent, Task, Crew,LLM\n",
    "from pydantic import BaseModel, Field\n",
    "from typing import List\n",
    "import json\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "5b299a0c",
   "metadata": {},
   "outputs": [],
   "source": [
    "llm = LLM(\n",
    "    model=\"groq/llama-3.3-70b-versatile\",\n",
    "    temperature=0.2,\n",
    "    api_key=os.getenv(\"GROQ_API_KEY\")\n",
    "    )"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "53272254",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Blog(BaseModel): # pydantic class \n",
    "    title: str\n",
    "    content: str"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "4e3a12bc",
   "metadata": {},
   "outputs": [],
   "source": [
    "blog_agent = Agent(\n",
    "    role=\"Blog Content Generator Agent\",\n",
    "    goal=\"Generate a blog title and content\",\n",
    "    backstory=\"\"\"You are an expert content creator, skilled in crafting engaging and informative blog posts.\"\"\",\n",
    "    verbose=True,\n",
    "    max_tokens = 200,\n",
    "    llm=llm,\n",
    ")\n",
    "\n",
    "task1 = Task(\n",
    "    description=\"\"\"\n",
    "    Topic: {topic}\n",
    "    Create a blog title and content on a given topic. Make sure the content is under 200 words.\"\"\",\n",
    "    expected_output=\"A compelling blog title and well-written content.\",\n",
    "    agent=blog_agent,\n",
    "    output_file=\"Output//output.json\", # the output will be saved in this file\n",
    "    create_directory=True, # create the directory if it does not exist\n",
    "    output_pydantic = Blog,\n",
    ")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "37970e7f",
   "metadata": {},
   "outputs": [],
   "source": [
    "crew = Crew(\n",
    "    agents=[blog_agent],\n",
    "    tasks=[task1],\n",
    "    verbose=True\n",
    ")"
   ]
  },
